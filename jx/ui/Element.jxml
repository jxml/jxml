<script>
	<![CDATA[
		function render(delta, attr) {
			var dirty = {};

			if ('text' in delta)
				dirty.text = delta.text;

			if ('background' in delta)
				dirty.background = delta.background;

			if ('x' in delta)
				dirty.x = delta.x;

			if ('y' in delta)
				dirty.y = delta.y;

			if ('width' in delta)
				dirty.width = delta.width;

			if ('height' in delta)
				dirty.height = delta.height;

			return dirty;
		}

		this.render = render;
	]]>
</script>
